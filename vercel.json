{
    "version": 2,
    "builds": [
      {
        "src": "dist/browser/**",
        "use": "@vercel/static"
      },
      {
        "src": "dist/server/server.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(?!api/)(.*)",
        "dest": "dist/server/server.mjs"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/dist/browser/assets/$1"
      }
    ]
  }