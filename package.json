{
  "name": "jemigo",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "start": "ng serve --proxy-config src/proxy.conf.json",
    "tsc": "tsc",
    "start-dev": "npm-run-all -p tsc client server",
    "client": "ng serve",
    "start:heroku": "node dist/jemigo/server/server.mjs",
    "start:ssr": "ng build && node dist/jemigo/server/server.mjs",
    "start:both": "npm-run-all -p start start:ssr",
    "start:client:term": "powershell -NoExit -Command \"npm run start\"",
    "start:ssr:term": "powershell -NoExit -Command \"npm run start:ssr\"",
    "start:both:terms": "npm-run-all -p start:client:term start:ssr:term",
    "postinstall": "patch-package",
    "heroku-cleanup": "patch-package"
  },
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/compiler-cli": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/language-service": "^20.3.1",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/platform-server": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/ssr": "^20.3.2",
    "@fortawesome/angular-fontawesome": "^3.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "axios": "^1.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-robots-txt": "^1.0.0",
    "helmet": "^7.1.0",
    "highlight.js": "^11.9.0",
    "mongoose": "^8.5.1",
    "ngx-highlightjs": "^11.0.0-beta.1",
    "ngx-sharebuttons": "^15.0.3",
    "node-schedule": "^2.1.1",
    "npm": "^10.9.0",
    "npm-run-all": "^4.1.5",
    "paapi5-typescript-sdk": "^0.2.0",
    "patch-package": "^8.0.0",
    "quill": "^2.0.3",
    "rxjs": "7.4",
    "sitemap": "^8.0.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular-eslint/builder": "^20.3.0",
    "@angular-eslint/eslint-plugin": "^20.3.0",
    "@angular-eslint/eslint-plugin-template": "^20.3.0",
    "@angular-eslint/schematics": "^20.3.0",
    "@angular-eslint/template-parser": "^20.3.0",
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
